{
	"name": "onedash-react-input-form",
	"version": "1.0.0",
	"description": " OneDash. React form and input components",
	"author": "ChewbaccaCookie",
	"license": "MIT",
	"repository": "OneDash-DE/onedash-react-input-form",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "microbundle --no-sourcemap --css-modules false --jsx React.createElement --format modern,cjs",
		"start": "microbundle watch --jsx React.createElement --css-modules false --no-compress --format modern,cjs",
		"build-example": "cd example && npm i && npm run build",
		"lint": "npx eslint src/**/*.{js,ts,tsx} --quiet --fix",
		"format": "npx prettier --write src/**/*.{js,ts,tsx}",
		"release": "npx semantic-release",
		"preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('npm/') && !console.log('Use npm to install dependencies in this repository\\n') && process.exit(1)\"",
		"install-all": "npm i && cd example && npm i"
	},
	"peerDependencies": {
		"react": "^16.13.1"
	},
	"devDependencies": {
		"@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
		"@semantic-release/git": "^9.0.0",
		"@types/cleave.js": "^1.4.3",
		"@types/node": "^14.11.8",
		"@types/react": "^16.9.52",
		"@types/react-dates": "^21.8.0",
		"@types/react-dom": "^16.9.8",
		"@types/react-select": "^3.0.22",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"babel-eslint": "^10.1.0",
		"cleave.js": "^1.6.0",
		"conventional-changelog-conventionalcommits": "^4.4.0",
		"cross-env": "^7.0.2",
		"dayjs": "^1.9.1",
		"eslint": "^7.11.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-config-standard": "^15.0.1",
		"eslint-config-standard-react": "^9.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.21.4",
		"eslint-plugin-standard": "^4.0.1",
		"husky": "^4.3.0",
		"microbundle": "^0.12.4",
		"moment": "^2.29.1",
		"node-sass": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"react": "^16.13.1",
		"react-dates": "^21.8.0",
		"react-dom": "^16.13.1",
		"react-scripts": "^4.0.0-next.98",
		"react-select": "^3.1.0",
		"semantic-release": "^17.2.1",
		"typescript": "^4.0.3",
		"uuid": "^8.3.1"
	},
	"files": [
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-merge": "npm run install-all"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}
